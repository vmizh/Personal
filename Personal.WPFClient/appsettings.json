{
  "Serilog": {
    "MinimumLevel": "Information",
    "Override": {
      "Microsoft.AspNetCore": "Warning"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs\\Logs.log"
        }
      }
    ]
  },
  "RedisCache": {
    "ConnectionString": "localhost:6379",
    "Database": "0"
  }
}
